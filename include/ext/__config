#pragma once
#pragma GCC system_header

#include <__config>

#define _LIBCPP_BEGIN_NAMESPACE_EXT namespace std { namespace ext {
#define _LIBCPP_END_NAMESPACE_EXT  } }
#define _VSTD_EXT std::ext

#define _LIBCPP_BEGIN_NAMESPACE_EXTBIT namespace std { namespace extbit { inline namespace __1 {
#define _LIBCPP_END_NAMESPACE_EXTBIT  } } }
#define _VSTD_EXTBIT std::extbit::__1

#define _LIBCPP_BEGIN_NAMESPACE_EXTBIT_V2 namespace std { namespace extbit { inline namespace __2 {
#define _LIBCPP_END_NAMESPACE_EXTBIT_V2  } } }
#define _VSTD_EXTBIT_V2 std::extbit::__2

#define _LIBCPP_BEGIN_NAMESPACE_EXTBIT_NET _LIBCPP_BEGIN_NAMESPACE_EXTBIT namespace net {
#define _LIBCPP_END_NAMESPACE_EXTBIT_NET  } } } }
#define _VSTD_EXTBIT_NET _VSTD_EXTBIT::net

#define _LIBCPP_BEGIN_NAMESPACE_EXTBIT_LOG _LIBCPP_BEGIN_NAMESPACE_EXTBIT namespace log {
#define _LIBCPP_END_NAMESPACE_EXTBIT_LOG  } } } }
#define _VSTD_EXTBIT_LOG _VSTD_EXTBIT::log
